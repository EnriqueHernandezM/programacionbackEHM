<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
      crossorigin="anonymous"
    />
  </head>
  <body class="bg-dark text-white">
    <%- include('../partials/navbar'); %>
    <div class="container m-auto">
      <div class="row">
        <% stre.forEach((item)=>{ %>
          <article class="mt-5 pt-5 d-flex align-items-center flex-row-reverse justify-content-around w-95 m-auto mb-5">
            <p class="w-25 text-center fs-2"> <%=item.producto%><br /></p>
            <p class="w-25 text-left fs-2">$: <%=item.precio%><br /></p>
            <img class="w-25 p-5 align me-auto" src=" <%=item.imagen %>" /><br />
            <p class="w-25 text-left fs-5">description%: <%=item.description%><br /></p>
          </article>
          <% }) %>
    </div>
    <%- include('../partials/footer'); %>
  </body>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
    crossorigin="anonymous"
  ></script>
</html>
